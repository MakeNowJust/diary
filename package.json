  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop --open",
    "format": "prettier-package-json 'package.json' 'plugins/*/package.json' --write && prettier --ignore-path .gitignore '**/*.{js,json,md,scss,yml}' '!src/posts/**' --write && eslint --ignore-path .gitignore '**/*.js' --fix && stylelint --ignore-path .gitignore '**/*.scss' --fix",
    "lint": "prettier-package-json 'package.json' 'plugins/*/package.json' --list-different && prettier --ignore-path .gitignore '**/*.{js,json,md,scss,yml}' '!src/posts/**' --list-different && eslint --ignore-path .gitignore '**/*.js' && stylelint --ignore-path .gitignore '**/*.scss'",
