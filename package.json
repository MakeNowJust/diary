    "modern-normalize": "1.1.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-instantsearch-dom": "6.16.0",
    "typeface-nova-mono": "1.1.13",
    "typeface-nova-square": "1.1.13",
    "typeface-raleway": "1.1.13"
  },
  "devDependencies": {
